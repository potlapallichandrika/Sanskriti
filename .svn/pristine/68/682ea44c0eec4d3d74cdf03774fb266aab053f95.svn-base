/*---- testimonials-----*/
.testimonial {
  background-color: #f7f9fb;
  position: relative;
  padding-top: 20px;
  padding-left: 10px;
  padding-bottom: 20px;
}
 .testimonials {
    margin-top: 50px;
    margin-bottom: 25px;
}
.testimonial .clearfix .testimonial-heading {
    display: inline-block;
    font-family: inherit;
    font-size: 105%;
    font-style: italic;
    font-weight: 600;
    line-height: 1.5;
    margin-top: 0;
    margin-bottom: 0;
    margin-right: 5px;
	padding-top: 20px;
    padding-bottom: 15px;
	
	}
.testimonial .clearfix .content {
    font-size: 100%;
    font-style: italic;
    font-weight: normal;
    line-height: 1.5;
    margin-top: 0px;
    margin-bottom: 20px;
	word-wrap: break-word;
   }
.testimonials-main-card{
	padding-bottom: 20px;
}
.clearfix::before, .clearfix::after {
    content: " ";
    display: table;
    clear: both;
}
.testimonials-quote{
	position: absolute;
	top: 5%;
    right: 10%;
	color: rgba(0,0,0,0.15);
	font-size: 300%;
    font-weight: 900;
}


/*---- winners css-----*/
.table.dataTable td {
  border: 1px solid rgba(0, 0, 0, 0.3);
  color: #666666;
  border-color: #eeeeee;
}

.table.dataTable thead th, table.dataTable thead td {
  border-color: #eeeeee;
  border-width: 1px 1px 1px 1px;
  border-style: solid;
  text-align: left;
}

.table.dataTable.no-footer {
  border: 1px solid rgba(0, 0, 0, 0.3);
  border-color: #eeeeee;
}

.table>thead {
  background-color: #fbfbfb;
}

.dataTables_filter {
  padding-bottom: 10px;
}

.winners-heading-title {
  text-align: center;
  padding: 20px;
  color: #ffffff;
  font-size: 40px;
}

#winners {
  background-color: #203864;
}

#winner-top-sec {
  background-color: #203864;
}

.button-link {
  font-family: "Source Serif Pro", Sans-serif;
  font-size: 14px;
  fill: #000000;
  color: #000000;
  background-color: #ffc5b3;
  border-style: solid;
  border-color: #ffc5b3;
  border-radius: 5px 5px 5px 5px;
  padding: 15px 15px 15px 15px;
}
.elementor-shape-fill {
  fill: #203864;
}
.winner-svg {
  display: block;
  width: calc(100% + 1.3px);
  position: relative;
  left: 50%;
  -webkit-transform: translateX(-50%);
  -ms-transform: translateX(-50%);
  transform: translateX(-50%);
}
.elementor-shape-top {
  overflow: hidden;
  position: absolute;
  left: 0;
  width: 100%;
  line-height: 0;
  direction: ltr;
}
