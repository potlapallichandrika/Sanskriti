{% set block_id =  'carouselExampleIndicators' ~ elements.content['#block_content'].id() %}
<div id="{{ block_id }}" class="carousel slide" data-bs-ride="carousel"style="margin-bottom: 20px;">
	<ol class="carousel-indicators">
	   {% for row in content.field_glimpses_slider['#items'] %}
		  {% set active_class = '' %}
		  {% set true_class = '' %}
		  {% if(loop.first) %}
		  {% set active_class = 'active' %}
		  {% set true_class = 'true' %}
		  {% endif %}
		  <li class="{{ active_class }}" data-bs-target="#{{ block_id }}" data-bs-slide-to="{{ loop.index0 }}" aria-current="{{ true_class }}" aria-label="Slide {{ loop.index0 }}"></li>
		  {% endfor %}	
	</ol>
	<div class="carousel-inner">
	{% for temp in content.field_glimpses_slider['#items'] %}
	  {% set active1 = '' %}
	  {% if loop.first %}
		{% set active1 = 'active' %}
	  {% else %}
		{% set active1 = '' %}
	  {% endif %}
		<div class="carousel-item {{active1}}"> <img src="{{ file_url(temp.entity.field_image.entity.uri.value) }}" class="d-block w-100" alt="{{ temp.entity.field_image.alt }}"> 
		
		</div>

	   {% endfor %}
	</div>
	<button class="carousel-control-prev" type="button" data-bs-target="#{{ block_id }}" data-bs-slide="prev"> <span class="carousel-control-prev-icon" aria-hidden="true"></span> <span class="visually-hidden">Previous</span> </button>
	<button class="carousel-control-next" type="button" data-bs-target="#{{ block_id }}" data-bs-slide="next"> <span class="carousel-control-next-icon" aria-hidden="true"></span> <span class="visually-hidden">Next</span> </button>
</div>
                    