<section class="elementor-section elementor-top-section elementor-element elementor-element-0346ca8 elementor-section-full_width elementor-section-height-default elementor-section-height-default" data-id="0346ca8" data-element_type="section" id="testimonials">
					<div class="elementor-container elementor-column-gap-default">
				<div class="elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-49e71a4" data-id="49e71a4" data-element_type="column">
		<div class="elementor-widget-wrap elementor-element-populated">
			<div class="elementor-element elementor-element-3f8ddae elementor-widget elementor-widget-heading" data-id="3f8ddae" data-element_type="widget" data-widget_type="heading.default">
			<div class="elementor-widget-container">
		<h1 class="elementor-heading-title elementor-size-default">Testimonials</h1>		
		
		</div>
			</div>
			
			<section class="elementor-section elementor-inner-section elementor-element elementor-element-acaa41b elementor-section-boxed elementor-section-height-default elementor-section-height-default" data-id="acaa41b" data-element_type="section">
				<div class="elementor-container elementor-column-gap-default">
				{% for row in content.field_testimonials['#items'] %}
	<div class="elementor-column elementor-col-33 elementor-inner-column elementor-element elementor-element-bfc4b65" data-id="bfc4b65" data-element_type="column">
		<div class="elementor-widget-wrap elementor-element-populated">
			<div class="elementor-element elementor-element-987a2b1 elementor-widget elementor-widget-eael-testimonial" data-id="987a2b1" data-element_type="widget" data-widget_type="eael-testimonial.default">
			<div class="elementor-widget-container">
		
        <div id="eael-testimonial-987a2b1" class="eael-testimonial-item clearfix  content-top-icon-title-inline">
				<div class="eael-testimonial-content eael-testimonial-inline-bio">
						<div class="eael-testimonial-image">
								<figure><img width="262" height="300" src="{{ file_url(row.entity.field_image.entity.uri.value) }}" class="attachment-medium size-medium" alt="" loading="lazy" /></figure>
						</div>
						<div class="bio-text"><p class="eael-testimonial-user">{{ row.entity.field_title.value }}</p></div>
						</div>
		<div class="eael-testimonial-content">
			<div class="eael-testimonial-text"><p>{{ row.entity.field_description.value | raw }}</p>
</div>			</div>
	
	
	<span class="eael-testimonial-quote"></span>
</div>

		</div>
			</div>
				</div>
	</div>
	{% endfor %}
	
						</div>
	</section>
	
				</div>
	</div>
						</div>
	</section>